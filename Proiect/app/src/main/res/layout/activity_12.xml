<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity12">

    <include layout="@layout/toolbar_activitati"></include>

    <CalendarView
        android:id="@+id/act12_calendar"
        android:layout_width="306dp"
        android:layout_height="297dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp" />

    <ImageButton
        android:id="@+id/act12_rapStudentiBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/act12_calendar"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-16dp"
        android:layout_marginBottom="154dp"
        app:srcCompat="@drawable/studentirap" />

    <TextView
        android:id="@+id/act12_rapStudentiTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="97dp"
        android:layout_marginBottom="175dp"
        android:text="@string/act12_rapStudentiText"
        android:textStyle="bold"/>

    <ImageButton
        android:id="@+id/act12_rapGrupeBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/act12_rapStudentiTV"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-175dp"
        android:layout_marginEnd="20dp"
        app:srcCompat="@drawable/gruperap" />

    <TextView
        android:id="@+id/act12_rapGrupeTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/act12_rapStudentiBtn"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-154dp"
        android:layout_marginEnd="85dp"
        android:text="@string/act12_rapGrupeText"
        android:textStyle="bold"/>

    <ImageButton
        android:id="@+id/act12_rapTesteBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/act12_rapGrupeTV"
        android:layout_alignStart="@+id/act12_rapStudentiBtn"
        app:srcCompat="@drawable/testerap" />

    <TextView
        android:id="@+id/act12_rapTesteTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/act12_rapGrupeBtn"
        android:layout_alignStart="@+id/act12_rapStudentiTV"
        android:text="@string/act12_rapTesteText"
        android:textStyle="bold"/>

    <ImageButton
        android:id="@+id/act12_rapCategTesteBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/act12_rapTesteTv"
        android:layout_alignEnd="@+id/act12_rapGrupeBtn"
        app:srcCompat="@drawable/categorietesterap" />

    <TextView
        android:id="@+id/act12_rapCategTesteTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/act12_rapTesteBtn"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="85dp"
        android:text="@string/act12_rapCategTesteText"
        android:textStyle="bold"/>


</RelativeLayout>